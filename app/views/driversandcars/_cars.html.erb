<p><strong><%= driver.name %>'s</strong> cars</p>
<table>
  <tr>
    <th>id</th>
    <th>title</th>
    <th>created at</th>
  </tr>
  <% driver.cars.each do |car| %>
    <tr>
      <td><%= car.id %></td>
      <td><%= car.title %></td>
      <td><%= car.created_at %></td>
    </tr>
  <% end %>
</table>
