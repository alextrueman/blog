<h1>Listing Cars</h1>
<%= link_to 'New Car', new_car_path %>

<table id='cars' border=1>
  <tr>
    <th>Title</th>
    <th>Type</th>
    <th>Color</th>
    <th></th>
  </tr>
 
  <% @cars.each do |car| %>
    <tr>
      <td><%= car.title %></td>
      <td><%= car.car_type %></td>
      <td><%= car.color %></td>
      <td><%= link_to 'Show', car_path(car) %></td>
    </tr>
  <% end %>
</table>
